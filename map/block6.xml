		<Obj x="380" y="80" w="60" h="30" type="6" img="block/unBlock.png" />
		<Obj x="380" y="110" w="60" h="30" type="6" img="block/unBlock.png" />
		<Obj x="380" y="140" w="60" h="30" type="6" img="block/unBlock.png"/>
		<Obj x="380" y="170" w="60" h="30" type="6" img="block/unBlock.png" />
		<Obj x="380" y="200" w="60" h="30" type="6" img="block/unBlock.png" />
		<Obj x="380" y="230" w="60" h="30" type="6" img="block/unBlock.png" />
		
		<Obj x="140" y="80" w="60" h="30" type="6" img="block/unBlock.png" />
		<Obj x="140" y="200" w="60" h="30" type="6" img="block/unBlock.png" />
		<Obj x="140" y="170" w="60" h="30" type="6" img="block/unBlock.png" />
		
		<Obj x="320" y="230" w="60" h="30" type="6" img="block/unBlock.png" />
