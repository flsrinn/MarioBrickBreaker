		<Obj x="200" y="230" w="60" h="30" type="6" img="block/unBlock.png" />
		<Obj x="140" y="230" w="60" h="30" type="6" img="block/unBlock.png" />
		
		<Obj x="320" y="200" w="60" h="30" type="5" img="block/mysteryBlock.png" />
	</Block>
	<Player>
		<Ball>
			<Obj size="1" balls="2" speed="2" img="player/shell.png"/>
		</Ball>
		<Paddle>
			<Obj x="100" y="475" w="50" h="50" type="1"  img="player/paddle.png" />
		</Paddle>
		<Shooter>
			<Obj x="148" y="510" w="50" h="40" type="1" img="player/shooter.png"/>
		</Shooter>
		<Bullet>
			<Obj w="60" h="40" type="1" img="player/bullet.png"/>
		</Bullet>
	</Player>
</GamePanel>
<ScorePanel>
	<Life>
		<Music>audio/decrease.wav</Music>
		<Heart>
			<Obj life="3" img="item/heart.png"/>
		</Heart>
	</Life>
</ScorePanel>
<ItemPanel>
	<Background>
		<BgImg>background/animalBackground.png</BgImg>
	</Background>
</ItemPanel>
<GameOverPanel>
	<Background>
		<BgImg>background/background1.png</BgImg>
		<Music>audio/gameover.wav</Music>
	</Background>
</GameOverPanel>
<GameClearPanel>
	<Background>
		<BgImg>background/animalBackground.png</BgImg> 
		<Music>audio/gameclear.wav</Music>	
	</Background>
</GameClearPanel>
</BlockBreakGame>