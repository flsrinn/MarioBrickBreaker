		<Obj x="430" y="90" w="60" h="30" type="1" img="block/block.png" />
		
		<Obj x="70" y="60" w="60" h="30" type="2" img="block/block.png"/>
		<Obj x="130" y="60" w="60" h="30" type="2" img="block/block.png"/>
		<Obj x="190" y="60" w="60" h="30" type="2"  img="block/block.png" />
		<Obj x="310" y="60" w="60" h="30" type="2"  img="block/block.png" />
