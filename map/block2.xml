		<Obj x="190" y="120" w="60" h="30" type="6" img="block/unBlock.png"/>
	</Block>
	<Player>
		<Ball>
			<Obj size="2" w="30" h="30" balls="2" speed="3" img="player/shell.png"/>
		</Ball>
		<Paddle>
			<Obj x="130" y="475" w="60" h="50" type="1"  img="player/paddle.png" />
		</Paddle>
		<Shooter>
			<Obj x="148" y="510" w="50" h="40" type="1" img="player/shooter.png"/>
		</Shooter>
		<Bullet>
			<Obj w="60" h="40" type="1" img="player/bullet.png"/>
		</Bullet>
	</Player>
</GamePanel>
<ScorePanel>
	<Life>
		<Music>audio/decrease.wav</Music>
		<Heart>
			<Obj life="4" img="item/heart.png"/>
		</Heart>
	</Life>
</ScorePanel>
<ItemPanel>
	<Background>
		<BgImg>background/animalBackground.png</BgImg>
	</Background>
</ItemPanel>
<GameOverPanel>
	<Background>
		<BgImg>background/background1.png</BgImg>
		<Music>audio/gameover.wav</Music>
	</Background>
</GameOverPanel>
<GameClearPanel>
	<Background>
		<BgImg>background/animalBackground.png</BgImg> 
		<Music>audio/gameclear.wav</Music>	
	</Background>
</GameClearPanel>
</BlockBreakGame>
