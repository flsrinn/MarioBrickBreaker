		<Obj x="430" y="60" w="60" h="30" type="2"  img="block/block.png" />
		
		<Obj x="190" y="90" w="60" h="30" type="3" img="block/block.png"/>
		<Obj x="250" y="90" w="60" h="30" type="3" img="block/block.png"/>
		<Obj x="310" y="90" w="60" h="30" type="3" img="block/block.png"/>
		<Obj x="370" y="90" w="60" h="30" type="3" img="block/block.png"/>
		<Obj x="310" y="120" w="60" h="30" type="3" img="block/block.png"/>
		<Obj x="250" y="150" w="60" h="30" type="3" img="block/block.png"/>
		
		<Obj x="250" y="120" w="60" h="30" type="4" img="block/block.png"/>
		
		<Obj x="130" y="90" w="60" h="30" type="5" img="block/mysteryBlock.png"/>
		<Obj x="310" y="150" w="60" h="30" type="5" img="block/mysteryBlock.png" />
