<?xml version="1.0" encoding="euc-kr" ?> 
<BlockBreakGame>
<GameScreen>
	<ScreenSize w="800" h="600"></ScreenSize>
</GameScreen>
<StartPanel>
	<Background>
		<BgImg>background/background1.png</BgImg>
		<Music>audio/start.wav</Music>
	</Background>
</StartPanel>
<GamePanel>
	<Background>
		<BgImg>background/background1.png</BgImg>
		<Music>audio/themesong.wav</Music>	
